<div class="media" id="comment{{ commentId }}">
	<a class="pull-left" href="#">
		<img class="media-object" src="http://placehold.it/64x64" alt="">
	</a>
	<div class="media-body">
		<h4 class="media-heading">{{ name }}
			<small>{{ date }}</small>
		</h4>
		{{ comment }}<br>
		<small><i> - <a onclick="comment_reply_form({{ parentId }}, {{ commentId }})">Svar</a>
			 | <a onclick="comment_remove({{ commentId }})">Fjern</a>
		</i></small>
	</div>
</div>